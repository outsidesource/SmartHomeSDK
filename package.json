{
  "name": "@outsidesource/smart-home-sdk",
  "version": "1.6.3",
  "description": "Alexa Smart Home Skill SDK",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --build tsconfig.json --pretty",
    "test": "yarn compile && NODE_ENV=test nyc mocha -r ts-node/register 'test/**/*.spec.ts'",
    "lint-check": "gts check",
    "lint-clean": "gts clean",
    "lint-fix": "gts fix",
    "version": "yarn version --no-git-tag-version",
    "test-check": "yarn clean && yarn test && yarn lint-check",
    "build": "yarn clean && yarn compile && cpy package.json dist && cd dist/ && yarn install --production"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/outsidesource/SmartHomeSDK.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "author": "Outside Source",
  "license": "UNLICENSED",
  "dependencies": {
    "ask-sdk-runtime": "^2.10.2",
    "aws-lambda": "^1.0.7",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.115",
    "@types/chai": "^4.3.3",
    "@types/lodash": "^4.14.185",
    "@types/mocha": "^9.1.1",
    "@types/node": "^20.2.5",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "chai": "^4.3.6",
    "check-error": "^1.0.2",
    "cpy-cli": "^4.2.0",
    "eslint": "^8.24.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.1",
    "gts": "^3.1.1",
    "mocha": "^10.0.0",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.spec.ts",
      "test"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  },
  "bugs": {
    "url": "https://github.com/outsidesource/SmartHomeSDK/issues"
  },
  "homepage": "https://github.com/outsidesource/SmartHomeSDK#readme",
  "directories": {
    "test": "test"
  }
}
